<div class="container">
    <table class="table table-dark table-hover mb-0">
        <thead class="thead-light">
            <tr>
                <th scope="col"> # </th>
                <th scope="col">Name</th>
                <th scope="col">Rank</th>
                <th scope="col">User Name</th>
            </tr>
        </thead>
        <tbody>
            <tbody>
                <% @users.each_with_index do |user,i|%>
                    <tr>
                        <th scope="row">
                            <%= i+1%>
                        </th>
                        <td>
                            <%=user.fname.capitalize%>
                        </td>
                        <td>
                            <%=user.rank.capitalize%>
                        </td>
                        <td>
                            <%= link_to user.username.capitalize, user_path(user), class:"btn btn-primary text-white text-capitalize"%>
                        </td>
                    </tr>
                    <% end %>
            </tbody>


    </table>

